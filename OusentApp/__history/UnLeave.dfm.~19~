  object adt: TADODataSet
    Active = True
    Connection = dm_DaTa.adoconn
    CursorType = ctStatic
    CommandText = 
      ' select   c.PCode,c.PName,b.DeptName,d.Name,a.*  FROM '#13#10'Hrm_Leav' +
      'e a  left   join   Hrm_Dept b   on   a.DeptId=b.DeptId'#13#10'left   j' +
      'oin   Hrm_Person c   on   a.PersonId=c.PersonId'#13#10'left   join    ' +
      'Hrm_LeaveType d  on  a.LeaveType=d.TypeId'#13#10'  WHERE  DateDiff(dd,' +
      'a.OnDate,getdate())=0'
